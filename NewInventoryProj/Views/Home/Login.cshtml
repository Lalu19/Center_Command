@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
 


<!DOCTYPE html>
<html>
<!-- Mirrored from foxythemes.net/preview/products/maisonnette/pages-login.php?theme=blue-sky by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 27 Nov 2020 04:38:09 GMT -->
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="/assets/img/favicon.html">
 
    @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>*@

    <script src="~/assets/ajaxapi.js"></script>

    <script src="~/Scripts/LoginScript.js"></script>
</head>
<body class="">

    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                Login to Continue

            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                <li><a href="#">Login</a></li>
                <li class="active">Login to Continue</li>
            </ol>
        </section>

        <!-- Main content -->
        <section class="content">
            <div class="row">

                <div class="col-md-6" style="margin-left: 25%;">
                    <!-- Horizontal Form -->
                    <div class="box box-info">
                        <div class="box-header with-border">
                            <h3 class="box-title">Login </h3>
                        </div>
                        <!-- /.box-header -->
                        <!-- form start -->
                        <form class="form-horizontal">
                            <div class="box-body">
                                <div class="form-group">
                                    <label for="inputEmail3" class="col-sm-4 control-label">Emp ID :</label>

                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" id="Personal_No" name="Personal_No" placeholder="Employee ID">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="inputPassword3" class="col-sm-4 control-label">Password :</label>

                                    <div class="col-sm-8">
                                        <input type="password" class="form-control" id="Password" name="Password" placeholder="Password">
                                    </div>
                                </div>

                            </div>
                            <!-- /.box-body -->
                            <div class="box-footer">
                                <a href="http://10.10.56.110:9090/tsbsl/sharedservices/welcome"> <button type="button" class="btn btn-default">Cancel</button></a>
                                <button type="button" id="Login" name="Login" class="btn btn-info pull-right">Sign in</button>
                            </div>
                            <!-- /.box-footer -->
                        </form>
                    </div>
                    <!-- /.box -->

                </div>
                <!--/.col (right) -->
            </div>
            <!-- /.row -->
        </section>
        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
    <script>
        //Enterkey enabled
        $(document).ready(function () {
            $('#Personal_No, #Password').keypress(function (e) {
                if (e.which == 13) { // 13 is the Enter key
                    $('#Login').click();
                }
            });
        });
    </script>
</body>

</html>